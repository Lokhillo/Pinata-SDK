{
  "compilerOptions": {
    "declaration": true,
    
   
    "jsx": "react-jsx", 

    "skipLibCheck": true,
    "sourceMap": true,
    /* Strict Type-Checking Options */
    "strict": true,
    "outDir": "types",
    "emitDeclarationOnly": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
   
    /* Module Resolution Options */
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
  },
  "include": ["src"]
}
{
    "name": "@pinata/sdk",
    "version": "2.1.0",
    "description": "Official SDK for the Pinata IPFS platform",
    "main": "lib/pinata-sdk.min.js",
    "types": "types/src/index.d.ts",
    "scripts": {
        "build": "npm run build-prod",
        "build-dev": "webpack --env dev",
        "build-prod": "webpack --env build",
        "coverage": "jest --coverage",
        "dev": "webpack --progress --colors --watch --env dev",
        "html": "open coverage/index.html",
        "lint": "eslint src --quiet ",
        "lint:fix": "eslint src test --fix",
        "test": "./node_modules/.bin/jest --env=node",
        "repl": "node -i -e \"$(< ./lib/pinata-sdk.js)\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PinataCloud/Pinata-SDK.git"
    },
    "keywords": [
        "webpack",
        "es6",
        "starter",
        "library",
        "universal",
        "umd",
        "commonjs"
    ],
    "author": "Matt Ober",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/PinataCloud/Pinata-SDK/issues"
    },
    "homepage": "https://github.com/PinataCloud/Pinata-SDK",
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/register": "^7.18.9",
        "@types/jest": "^29.4.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "acorn": "^8.8.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^9.1.2",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-polyfill": "^6.26.0",
        "eslint": "^8.34.0",
        "eslint-webpack-plugin": "^4.0.0",
        "jest": "^29.4.2",
        "source-map-loader": "^4.0.1",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "buffer": "^6.0.3",
        "form-data": "^4.0.0",
        "is-ipfs": "^0.6.0",
        "path": "^0.12.7",
        "pino": "^8.10.0",
        "process": "^0.11.10"
    },
    "browser": {
        "fs": false,
        "os": false,
        "path": false
    },
    "jest": {
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ]
    },
    "workspaces": [
        "packages/*"
    ]
}
